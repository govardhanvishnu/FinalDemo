<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL( Prefered_contact_Type__c , &#39;Email&#39;), ISBLANK( Email_Id__c )   )</errorConditionFormula>
    <errorMessage>Enter Email-Id</errorMessage>
</ValidationRule>
